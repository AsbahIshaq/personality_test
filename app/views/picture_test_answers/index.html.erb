<body>
<button class="">
  <%= link_to 'Objective Tests', objective_test_answers_path %>
</button>
<button class="">
  <%= link_to 'Picture Tests', picture_test_answers_path %>
</button>
<form class="jotform-form" action= <%= answer_submission_picture_test_answers_path %> method="post" accept-charset="utf-8" autocomplete="on">
  <div role="main" class="form-all">
    <ul class="form-section page-section">
      <li id="cid_1" class="form-input-wide" data-type="control_head">
        <div class="form-header-group  header-large">
          <div class="header-text httal htvam">
            <h1 id="header_1" class="form-header" data-component="header">
              Personality Test
            </h1>
            <div id="subHeader_1" class="form-subHeader">
              Please take a few moments to complete this test
            </div>
          </div>
        </div>
      </li>
      </li>
      <li class="form-line" data-type="control_dropdown" id="id_3">
        <label class="form-label form-label-top form-label-auto" id="label_3" for="input_3"> Name </label>
        <div id="cid_3" class="form-input-wide">
          <%= text_field_tag 'user_name', nil, required: true %>
        </div>
      </li>
      <li class="form-line" data-type="control_birthdate" id="id_5">
        <label class="form-label form-label-top form-label-auto" id="label_5" for="input_5"> Email Address </label>
        <div id="cid_5" class="form-input-wide">
          <%= text_field_tag 'email_address', nil, required: true %>
        </div>
      </li>
      <li class="form-line" data-type="control_matrix" id="id_4">
        <label class="form-label form-label-top" id="label_4" for="input_4"> Rate the picture </label>
        <div id="cid_4" class="form-input-wide">
          <table summary="" role="presentation" cellPadding="4" cellSpacing="0" class="form-matrix-table" data-component="matrix">
            <tbody>
              
              <% ObjectiveTest.all.each_with_index do |test, i| %>
                <tr role="group" aria-labelledby="label_4 label_4_row_0">
                  <div>
                    <th scope="row" class="form-matrix-row-headers">
                      <%= image_tag(image_path("picture_tests/#{i+1}.jpg"), size: "100", style: "text-align: center;") %>
                    </th>
                  <div/>
                  <div id="cid_28" class="form-input jf-required">
                    <span class="form-sub-label-container " style="vertical-align:top">
                      <table role="radiogroup" aria-labelledby="label_28 sublabel_input_28_description" cellpadding="4" cellspacing="0" class="form-scale-table" data-component="scale">
                        <tbody>
                          <tr>
                            <th>
                              &nbsp;
                            </th>
                            <th style="text-align:center">
                              <label for="input_28_1"> 1 </label>
                            </th>
                            <th style="text-align:center">
                              <label for="input_28_2"> 2 </label>
                            </th>
                            <th style="text-align:center">
                              <label for="input_28_3"> 3 </label>
                            </th>
                            <th style="text-align:center">
                              <label for="input_28_4"> 4 </label>
                            </th>
                            <th style="text-align:center">
                              <label for="input_28_5"> 5 </label>
                            </th>
                            <th>
                              &nbsp;
                            </th>
                          </tr>
                          <tr>
                            <td>
                              <label for="input_28_worst" aria-hidden="true"> Ugly </label>
                            </td>
                            <td style="text-align:center">
                              <input type="radio" id= <%= "input_#{test.id}_0_1" %> class="form-radio" name= <%= "questions[#{test.id}]" %> value="3" aria-labelledby="label_4_col_2" />
                            </td>
                            <td style="text-align:center">
                              <input type="radio" id= <%= "input_#{test.id}_0_2" %> class="form-radio" name= <%= "questions[#{test.id}]" %> value="3" aria-labelledby="label_4_col_2" />
                            </td>
                            <td style="text-align:center">
                              <input type="radio" id= <%= "input_#{test.id}_0_3" %> class="form-radio" name= <%= "questions[#{test.id}]" %> value="3" aria-labelledby="label_4_col_2" />
                            </td>
                            <td style="text-align:center">
                              <input type="radio" id= <%= "input_#{test.id}_0_4" %> class="form-radio" name= <%= "questions[#{test.id}]" %> value="3" aria-labelledby="label_4_col_2" />
                            </td>
                            <td style="text-align:center">
                              <input type="radio" id= <%= "input_#{test.id}_0_5" %> class="form-radio" name= <%= "questions[#{test.id}]" %> value="3" aria-labelledby="label_4_col_2" />
                            </td>
                            <td>
                              <label for="input_28_best" aria-hidden="true"> Beautiful </label>
                            </td>
                          </tr>
                        </tbody>
                      </table>
                      <label class="form-sub-label" id="sublabel_input_28_description" style="border:0;clip:rect(0 0 0 0);height:1px;margin:-1px;overflow:hidden;padding:0;position:absolute;width:1px;white-space:nowrap" aria-hidden="false"> 1 is Strongly Disagree, 5 is Strongly Agree </label>
                    </span>
                  </div>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </li>
      <li class="form-line" data-type="control_button" id="id_2">
        <div id="cid_2" class="form-input-wide">
          <div style="margin-left:156px" data-align="auto" class="form-buttons-wrapper  ">
            <%= submit_tag "Submit", class: "form-submit-button" %>
          </div>
        </div>
      </li>
      <li style="display:none">
        Should be Empty:
        <input type="text" name="website" value="" />
      </li>
    </ul>
  </div>
  <script>
  JotForm.showJotFormPowered = "new_footer";
  </script>
  <script>
  JotForm.poweredByText = "Powered by JotForm";
  </script>
  <input type="hidden" id="simple_spc" name="simple_spc" value="200742580758459" />
  <script type="text/javascript">
  document.getElementById("si" + "mple" + "_spc").value = "200742580758459-200742580758459";
  </script>
  <div class="formFooter-heightMask">
  </div>
  <div class="formFooter f6">
    <a href="https://www.jotform.com/?utm_source=formfooter&utm_medium=banner&utm_term=200742580758459&utm_content=jotform_logo&utm_campaign=powered_by_jotform_le" target="_blank" class="formFooter-logoLink"></a>
    <div class="formFooter-rightSide">
      <span class="formFooter-text">
        If you are an admin log in
      </span>
      <% if user_signed_in? %>
        <button class="formFooter-button">
          <%= link_to 'Sign out', destroy_user_session_path, method: :delete  %>
        </button>
      <% else %>
        <button class="formFooter-button">
          <%= link_to 'Sign in', new_user_session_path  %>
        </button>
      <% end %>
    </div>
  </div>
</form></body>
</html>
