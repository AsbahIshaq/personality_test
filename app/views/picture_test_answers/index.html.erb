   JotForm.prepareCalculationsOnTheFly([null,{"name":"clickTo","qid":"1","text":"Patient Satisfaction Survey","type":"control_head"},{"name":"submit","qid":"2","text":"Submit Survey","type":"control_button"},{"name":"gender","qid":"3","text":"Gender","type":"control_dropdown"},{"name":"overallSatisfaction","qid":"4","text":"Overall satisfaction","type":"control_matrix"},{"name":"birthDate5","qid":"5","text":"Birth Date","type":"control_birthdate"},{"name":"howCan","qid":"6","text":"How can we improve our service?","type":"control_textarea"}]);
   setTimeout(function() {
